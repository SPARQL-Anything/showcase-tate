PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX fx:   <http://sparql.xyz/facade-x/ns/>
PREFIX xyz:  <http://sparql.xyz/facade-x/data/>
PREFIX tate: <http://sparql.xyz/example/tate/>
PREFIX tsub: <http://sparql.xyz/example/tate/subject/>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?subjectId ?subjectName (COUNT(?artwork) as ?count)
WHERE {
	?artwork dct:subject ?subject .
	?subject rdfs:label ?subjectName .
	BIND(SUBSTR(str(?subject),STRLEN(STR(tsub:))) as ?subjectId)
} 
GROUP BY ?subjectId ?subjectName
ORDER BY DESC(?count)